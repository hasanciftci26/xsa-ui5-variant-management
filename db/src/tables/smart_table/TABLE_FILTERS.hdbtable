COLUMN TABLE "TABLE_FILTERS" (
	PROJECT_ID NVARCHAR(255),
	FILE_NAME NVARCHAR(50),
	USER_NAME NVARCHAR(255),
	PERSISTENCY_KEY NVARCHAR(255),
	COLUMN_KEY NVARCHAR(100),
	ITEM_ID NVARCHAR(36),
	OPERATION NVARCHAR(20),
	FIRST_VALUE NVARCHAR(255),
	SECOND_VALUE NVARCHAR(255),
	EXCLUDE BOOLEAN,
	PRIMARY KEY(PROJECT_ID,FILE_NAME,USER_NAME,PERSISTENCY_KEY,COLUMN_KEY,ITEM_ID)
)