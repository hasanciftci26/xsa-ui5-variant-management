COLUMN TABLE "FILTERBAR_ITEMS" (
	PROJECT_ID NVARCHAR(255),
	FILE_NAME NVARCHAR(50),
	USER_NAME NVARCHAR(255),
	PERSISTENCY_KEY NVARCHAR(255),
	KEY_FIELD NVARCHAR(100),
	KEY_VALUE NVARCHAR(255),
	FIELD_TEXT NVARCHAR(255),
	PRIMARY KEY(PROJECT_ID,FILE_NAME,USER_NAME,PERSISTENCY_KEY,KEY_FIELD,KEY_VALUE)
)