COLUMN TABLE "FILTERBAR_RANGES" (
	PROJECT_ID NVARCHAR(255),
	FILE_NAME NVARCHAR(50),
	USER_NAME NVARCHAR(255),
	PERSISTENCY_KEY NVARCHAR(255),
	KEY_FIELD NVARCHAR(100),
	RANGE_ID NVARCHAR(36),
	OPERATION NVARCHAR(20),
	FIRST_VALUE NVARCHAR(255),
	SECOND_VALUE NVARCHAR(255),
	EXCLUDE BOOLEAN,
	TOKEN_TEXT NVARCHAR(255),
	SINGLE_VALUE BOOLEAN,
	PRIMARY KEY(PROJECT_ID,FILE_NAME,USER_NAME,PERSISTENCY_KEY,KEY_FIELD,RANGE_ID)
)