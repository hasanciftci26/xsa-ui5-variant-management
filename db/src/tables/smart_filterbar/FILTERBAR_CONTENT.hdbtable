COLUMN TABLE "FILTERBAR_CONTENT" (
	PROJECT_ID NVARCHAR(255),
	FILE_NAME NVARCHAR(50),
	USER_NAME NVARCHAR(255),
	PERSISTENCY_KEY NVARCHAR(255),
	FIELD_NAME NVARCHAR(100),
	FILTER_GROUP NVARCHAR(100),
	PART_OF_CURRENT_VARIANT BOOLEAN,
	VISIBLE_IN_FILTERBAR BOOLEAN,
	VISIBLE BOOLEAN,
	PRIMARY KEY(PROJECT_ID,FILE_NAME,USER_NAME,PERSISTENCY_KEY,FIELD_NAME)
)