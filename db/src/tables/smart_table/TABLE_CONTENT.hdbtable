COLUMN TABLE "TABLE_CONTENT" (
	PROJECT_ID NVARCHAR(255),
	FILE_NAME NVARCHAR(50),
	USER_NAME NVARCHAR(255),
	PERSISTENCY_KEY NVARCHAR(255),
	COLUMN_KEY NVARCHAR(100),
	COLUMN_INDEX INTEGER,
	COLUMN_WIDTH NVARCHAR(15),
	COLUMN_VISIBLE BOOLEAN,
	PRIMARY KEY(PROJECT_ID,FILE_NAME,USER_NAME,PERSISTENCY_KEY,COLUMN_KEY)
)